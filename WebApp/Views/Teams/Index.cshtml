@model List<WebApp.Controllers.Team>
@{ ViewBag.Title = "Index"; }

<h2>Teams</h2>

<table>
  <thead>
    <tr>
      <th>Team</th>
      <th>Cups</th>
    </tr>
  </thead>
  <tbody>
    @foreach (var team in Model) {
      <tr>
        <td>
          <a href="~/teams/details/@team.Id">@team.Name</a>
        </td>
        <td>@team.Cups</td>
      </tr>
    }
  </tbody>
</table>